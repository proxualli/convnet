<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:ConvnetAvalonia.ViewModels"
        xmlns:view="clr-namespace:ConvnetAvalonia.PageViews"
        xmlns:v="clr-namespace:ConvnetAvalonia.PageViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mxb="clr-namespace:Eremex.AvaloniaUI.Controls.Bars;assembly=Eremex.Avalonia.Controls"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="ConvnetAvalonia.MainWindow"
        Icon="/Resources/App.ico"
        Title="Convnet Explorer">

    <Window.DataTemplates>
      <DataTemplate DataType="{x:Type v:PageViewModel}">
        <view:PageView />
      </DataTemplate>
    </Window.DataTemplates>
  
    <Design.DataContext>
         <vm:MainWindowViewModel/>
    </Design.DataContext>
  
    <view:PageView x:Name="MainView"/>
    <!--<TextBlock Text="{Binding Greeting}" HorizontalAlignment="Center" VerticalAlignment="Center"/>-->
</Window>
